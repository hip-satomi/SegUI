{
    "/tf/predictions/*": {
        "target": "http://localhost/",
        "secure": false,
        "logLevel": "debug",
        "changeOrigin": true
    },
    "/pt/predictions/*": {
        "target": "http://localhost/",
        "secure": false,
        "logLevel": "debug",
        "changeOrigin": true
    },
    "/mediafiles/*": {
        "target": "http://localhost/",
        "secure": false,
        "logLevel": "debug",
        "changeOrigin": true
    },
    "/omero/api/*": {
        "target": "http://localhost:4080/",
        "secure": false,
        "logLevel": "debug",
        "changeOrigin": true,
        "pathRewrite": {"^/omero/api/" : "/api/v0/"}
    },
    "/omero/webclient/*": {
        "secure": false,
        "logLevel": "debug",
        "changeOrigin": true
    }
}